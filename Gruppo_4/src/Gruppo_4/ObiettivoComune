package Gruppo_4;

public class ObiettivoComune {

boolean ObiettivoComune1(Integer[][] libreria) {
		int contatore=0;
		for(int i=0; i<4; i++) {
			for(int j=0; j<5; j++) {
				if(libreria[i][j]==libreria[i][j+1]&&libreria[i][j]==libreria[i+1][j]&&libreria[i][j]==libreria[i+1][j+1]) {
					contatore++;
				}
			}
		}
		if(contatore>=2) {
			return true;
		}
		else {
			return false;
		}
	}
}
